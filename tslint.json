{
  "defaultSeverity": "warning",
  "extends": [
    "tslint:recommended",
    "tslint-react"
  ],
  "rules": {
    "member-access": false,
    "object-literal-sort-keys": false,
    "ordered-imports": [false],
    "quotemark": [
      true,
      "single"
    ],
    "trailing-comma": [true, {
      "multiline": {
        "objects": "never"
      }
    }]
  }
}